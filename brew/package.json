{
  "name": "brew",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "codegen:watch": "nodemon --ext tsx --exec pnpm codegen",
    "codegen": "wget http://localhost:8000/schema -O ./schema.graphql && pnpm graphql-codegen",
    "preview": "vite preview"
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/introspection": "^4.0.0",
    "@mantine/core": "^7.1.7",
    "@mantine/form": "^7.1.7",
    "@mantine/hooks": "^7.1.7",
    "@tsly/core": "^1.0.0",
    "@tsly/hooks": "^1.1.0",
    "@tsly/maybe": "^1.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.5.3",
    "framer-motion": "^10.16.5",
    "graphql": "^16.8.1",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^4.11.0",
    "react-router-dom": "^6.17.0",
    "react-use-websocket": "^4.5.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.4"
  },
